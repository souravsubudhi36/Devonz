{
  "name": "devonz",
  "description": "An AI Agent",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "version": "1.0.0",
  "author": {
    "name": "Devonz team"
  },
  "scripts": {
    "deploy": "npm run build",
    "build": "remix vite:build",
    "dev": "node pre-start.cjs  && remix vite:dev",
    "dev:clean": "node pre-start.cjs && node -e \"require('fs').rmSync('node_modules/.vite',{recursive:true,force:true})\" && remix vite:dev",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app",
    "lint:fix": "npm run lint -- --fix && prettier app --write",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "preview": "pnpm run build && pnpm run start",
    "prepare": "husky",
    "clean": "node scripts/clean.js",
    "docker:build": "docker build -t devonz .",
    "docker:run": "docker run --rm -p 5173:5173 --env-file .env.local devonz",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:dev": "docker compose --profile dev up devonz-dev",
    "docker:update": "docker compose pull && docker compose up -d",
    "update": "node scripts/update.cjs"
  },
  "engines": {
    "node": ">=18.18.0"
  },
